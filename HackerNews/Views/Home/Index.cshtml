@{
    ViewBag.Title = "Home";
}

<div class="row">
    <div class="col-md-4">
        <h2>Top Stories</h2>
        <div ng-app="myApp" ng-controller="myCtrl">

            <div data-ng-init="init()">
                <ul>
                    <li ng-repeat="x in allData">
                        Author: {{ x.by }}, Title: {{ x.title }}
                    </li>
                </ul>
            </div>

        </div>
    </div>
</div>